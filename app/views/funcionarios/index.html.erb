<div class="row wrapper border-bottom white-bg" >
  <div class="row wrapper white-bg">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-content">
          <%= link_to 'Novo Funcionário', new_funcionario_path, data: { no_turbolink: true }, class: 'btn btn-w-m btn-primary'  %>
        </div>

      </div>
    </div>
  </div>
  <div class="row wrapper border-bottom white-bg">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-content">
          <table class="table table-striped table-bordered table-hover  dataTable" id="funcionarios-table", data-source="<%= funcionarios_path(format: :json) %>">
            <thead>
              <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Contacto</th>
                <th>Email</th>
                <th>Contribuinte</th>
                <th>Preço Hora</th>
                <th>Regularizado</th>
                <th>Opções</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
