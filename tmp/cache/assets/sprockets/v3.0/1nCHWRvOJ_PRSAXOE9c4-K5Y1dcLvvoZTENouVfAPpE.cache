        api = this.api();
        intVal = function(i) {
          if (typeof i === 'string') {
            return i.replace(/[^0-9^.]+/g, '') * 1;
          } else if (typeof i === 'number') {
            return i;
          } else {
            return 0;
          }
        };
        total = api.column(5).data().reduce((function(a, b) {
          return intVal(a) + intVal(b);
        }), 0);
        pageTotal = api.column(5, {
          page: 'current'
        }).data().reduce((function(a, b) {
          return intVal(a) + intVal(b);
        }), 0);
